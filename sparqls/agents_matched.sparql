PREFIX dbo: <http://dbpedia.org/ontology/>
PREFIX schema: <http://schema.org/>
select * where {
    graph <http://kg.artsdata.ca/culture-creates/artsdata-planet-iwts/iwts-ca> {
        ?s a ?type ;
        schema:name ?name .
        OPTIONAL {
            ?s   schema:url ?url .
        }
        OPTIONAL {
            ?s schema:location/schema:address/schema:addressCountry ?country .
        }
        OPTIONAL {
            ?s schema:workLocation/schema:address/schema:addressCountry ?country .
        }
    }
    ?s a dbo:Agent .
    OPTIONAL {
        graph <http://kg.artsdata.ca/core> {
            ?s schema:sameAs ?artsdata_uri .
        }
    }
    OPTIONAL {
        ?s schema:additionalType <http://kg.artsdata.ca/ontology/FlaggedForReview>
        BIND(true as ?is_flagged_for_review)
    }
}
